{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "[%(asctime)s] %(levelname)s %(name)s: %(message)s"
        },
        "simple": {
            "format": "%(message)s"
        }
    },
    "handlers": {
        "console_handler": {
            "level": "INFO",
            "formatter": "standard",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        },
        "file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "standard",
            "filename": "janrain-dataload.log",
            "maxBytes": 524288000
        },
        "success_handler": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "simple",
            "filename": "success.csv",
            "mode": "w"
        },
        "fail_handler": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "simple",
            "filename": "fail.csv",
            "mode": "w"
        },
        "uuid_handler": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "simple",
            "filename": "uuid.csv",
            "mode": "w"
        }
    },
    "loggers": {
        "success_logger": {
            "handlers": ["success_handler"],
            "propagate": false
        },
        "fail_logger": {
            "handlers": ["fail_handler"],
            "propagate": false
        },
        "uuid_logger":{
            "handlers": ["uuid_handler"],
            "propagate": false
        },
        "requests.packages.urllib3.connectionpool": {
            "level": "WARN"
        },
        "janrain.capture.api": {
            "level": "INFO"
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": ["console_handler", "file_handler"],
        "propogate": true
    }
}